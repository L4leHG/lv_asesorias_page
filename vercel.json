{
  "version": 2,
  "builds": [
    {
      "src": "php/procesar_cita.php",
      "use": "@vercel/php"
    }
  ],
  "routes": [
    {
      "src": "/php/procesar_cita.php",
      "dest": "/php/procesar_cita.php"
    },
    {
      "src": "/(.*\\.(html|css|js|jpg|jpeg|png|gif|svg|ico|woff|woff2|ttf|eot))",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}

